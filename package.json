{
  "name": "website-boilerplate",
  "description": "a boilerplate of website provided with npm scripts",
  "version": "2.0.0",
  "author": "tuqulore inc.",
  "bugs": {
    "url": "https://github.com/tuqulore/website-boilerplate/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "serve-handler": "^6.1.5"
  },
  "devDependencies": {
    "fast-glob": "^3.2.12",
    "@11ty/eleventy": "2.0.0",
    "@11ty/eleventy-img": "3.0.0",
    "@jumpu-ui/tailwindcss": "^1.0.1-alpha.5",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "10.4.13",
    "axios": "1.3.4",
    "cachekill": "^3.0.2",
    "cssnano": "^5.1.15",
    "dotenv": "^16.0.1",
    "eleventy-plugin-error-overlay": "1.0.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-formatter-rdjson": "1.0.5",
    "html-minifier": "4.0.0",
    "jsdom": "21.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-toc-done-right": "^4.2.0",
    "node-html-parser": "^6.1.5",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "prefixer": "^0.0.3",
    "prettier": "^2.8.4",
    "prettier-plugin-md-nocjsp": "^1.5.1",
    "rimraf": "4.3.0",
    "tailwindcss": "^3.2.7"
  },
  "homepage": "https://github.com/tuqulore/website-boilerplate#readme",
  "keywords": [
    "website"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuqulore/website-boilerplate.git"
  },
  "scripts": {
    "build": "NODE_ENV=production run-s clean build:{eleventy,style}",
    "build:eleventy": "eleventy",
    "build:cachekill": "cachekill -r -s 'docs/**/!(*.html)' -t 'docs/**/*.{js,css,html}'",
    "build:style": "postcss 'src/style/**/[^._]*' --dir docs/style --ext css",
    "clean": "rimraf docs",
    "dev": "run-p dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:style": "postcss 'src/style/**/[^._]*' --dir docs/style --watch --ext css",
    "fix": "eslint --fix .",
    "format": "prettier --verbose --write '**/*.{js,json,md,njk,css,scss,pcss,yaml}'",
    "lint": "eslint .",
    "lint:rdjson": "eslint --format rdjson ."
  }
}
